{
  "name": "books-hub-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:local": "dotenv -e .env.local -- next dev --turbopack",
    "serve:dev": "dotenv -e .env.development -- next dev --turbopack",
    "serve:prod": "dotenv -e .env.production -- next dev --turbopack",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
    "migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "build": "npx tsc --noEmit && npx prisma generate && npx next build",
    "start": "next start",
    "lint": "next lint",
    "seed:local": "dotenv -e .env.local -- prisma db seed",
    "seed:dev": "dotenv -e .env.development -- prisma db seed"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.716.0",
